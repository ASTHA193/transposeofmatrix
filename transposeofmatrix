#include<stdio.h>
void main()
{
    int a[3][3],i,j;
    printf("\n enter the elements of matrix");
    for(i=0;i<=2;i++)
    {
     for(j=0;j<=2;j++)
      {
       scanf("%d" ,&a[i][j]);
      }
    }


    printf("\nbefore transpose\n");
    for(i=0;i<=2;i++)
    {
     for(j=0;j<=2;j++)
      {
      printf("%d    ",a[i][j]);
      }
      printf("\n");
    }
    printf("\nafter transpose\n");
    for(i=0;i<=2;i++)
    {
     for(j=0;j<=2;j++)
      {
      printf("%d    ",a[j][i]);
      }
      printf("\n");
    }
}
